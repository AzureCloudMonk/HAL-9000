# https://docs.docker.com/machine/
if [[ $(docker-machine ip default 2>/dev/null) == "192.168."* ]]; then
  eval "$(docker-machine env default)";
fi

# delete all containers
alias docker-rmall='docker rm -f $(docker ps -a -q)'

# delete all images
alias docker-rmiall='docker rmi $(docker images -q)'

# delete all untagged images
alias docker-rminotag='docker rmi $(docker images -q -f dangling=true)'
